{
	"name": "@vyke/torao-monorepo",
	"type": "module",
	"version": "0.0.1",
	"packageManager": "pnpm@9.0.4",
	"description": "Small and type-friendly library for game development",
	"author": "Jose Albizures <jose@albizures.com> (https://albizures.com/)",
	"license": "MIT",
	"keywords": [
		"helpers",
		"typescript",
		"torao",
		"game",
		"gameengine",
		"engine"
	],
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./texture": {
			"import": "./dist/texture.mjs",
			"require": "./dist/texture.cjs"
		}
	},
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"turbo": "turbo",
		"build": "turbo build",
		"dev": "turbo dev:build dev",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"dependencies": {
		"turbo": "^2.0.12"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^2.22.4",
		"eslint": "^9.7.0",
		"knip": "^5.26.0",
		"npm-run-all": "^4.1.5",
		"typescript": "^5.5.3"
	},
	"standard-version": {
		"skip": {
			"changelog": true
		}
	}
}
