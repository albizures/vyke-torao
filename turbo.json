{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"outputs": ["dist/**"]
		},
		"dev:build": {
			"outputs": ["dist/**"]
		},
		"lint:types": {
			"dependsOn": ["^lint:types"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"test:unit:dev": {
			"persistent": true,
			"cache": false
		}
	}
}
